{
   "firestore": {
      "database": "(default)",
      "rules": "firestore.rules",
      "indexes": "firestore.indexes.json"
   },
   "hosting": {
      "public": "dist/szte-web-kert-2025-fake-neptun/browser",
      "ignore": [
         "firebase.json",
         "**/.*",
         "**/node_modules/**"
      ],
      "rewrites": [
         {
            "source": "**",
            "destination": "/index.html"
         }
      ]
   }
}
